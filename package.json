{
  "name": "mainframesearch",
  "displayName": "MainframeSearch",
  "description": "A search job extension to run on a mainframe",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mainframesearch.Search",
        "title": "Search Mainframe (3.14)"
      },
      {
        "command": "mainframesearch.teste",
        "title": "testa mainframe"
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "when": "view == zowe.ds.explorer && viewItem =~ /^(pds|ds|migr)/",
          "command": "mainframesearch.Search",
          "group": "100_mainframesearch"
        },
        {
          "command": "mainframesearch.testa",
          "group": "100_mainframesearch"
        }
      ]
    },
    "configuration": {
      "title": "Mainframe Search",
      "properties": {
        "mainframesearch.JobCard": {
          "type": "array",
          "description": "Job Card to be used on job search",
          "default": "//X93182SR JOB ,'Search',MSGCLASS=X,CLASS=D,REGION=6M",
          "order": 1,
          "minimum": 1
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "package": "yarn build && npm pack && node ../../scripts/mv-pack.js zowe-zowe-explorer-api tgz"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "@vscode/test-electron": "^2.3.4",
    "@zowe/zowe-explorer-api": "1.16.1"
  },
  "extensionDependencies": [
    "Zowe.vscode-extension-for-zowe"
  ],
  "dependencies": {
    "@zowe/zowe-explorer-api": "1.16.1"
  }
}